{
   "content_scripts": [ {
      "js": [ "autocomplete.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_start"
   },
   {
      "js": ["jquery-1.7.2.min.js", "goodreads.js"],
      "matches": ["http://www.goodreads.com/book/show/*"],
      "run_at": "document_end"
   }
   ],
   "description": "Various Chrome fixes",
   "icons": {
      "32": "icon32.png",
      "48": "icon48.png"
   },
   
   "name": "Chromeplus",
   "version": "0.0.1",
   "permissions": [ "http://*/*" ],
   "background_page": "background.html"
}
